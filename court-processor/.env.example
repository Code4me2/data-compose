# Court Processor Environment Variables
# Copy this to .env and update with your values

# Database connection (uses data-compose PostgreSQL)
# This should match your data-compose database settings
DATABASE_URL=postgresql://postgres:postgres@db:5432/postgres

# Logging level
LOG_LEVEL=INFO

# OCR settings
OCR_ENABLED=true

# Rate limiting (seconds between requests)
REQUEST_DELAY=2